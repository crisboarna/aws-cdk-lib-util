{
  "name": "aws-cdk-lib-util",
  "version": "1.6.1",
  "description": "Library providing utility functions for AWS CDK v2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/crisboarna/aws-cdk-lib-util.git"
  },
  "author": "Cristian Boarna <contact@crisboarna.com>",
  "bugs": {
    "url": "https://github.com/crisboarna/aws-cdk-lib-util/issues"
  },
  "homepage": "https://github.com/crisboarna/aws-cdk-lib-util",
  "files": [
    "dist",
    "README.md",
    "CONTRIBUTING.md",
    "CHANGELOG.md",
    "LICENSE.md",
    "package.json"
  ],
  "keywords": [
    "AWS CDK",
    "Cloudformation",
    "utility",
    "aws",
    "cdk",
    "v2"
  ],
  "scripts": {
    "lint": "eslint ./src/** --ext .ts --max-warnings 0",
    "test": "jest --coverage",
    "build": "tsc && cd ../../ && cp README.md LICENSE.md CONTRIBUTING.md packages/aws-cdk-lib-util",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@nrwl/jest": "14.7.5",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.6",
    "@types/jest": "27.0.2",
    "@types/node": "18.7.16",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.37.0",
    "aws-cdk-lib": "^2.21.1",
    "constructs": "^10.1.161",
    "dotenv": "^16.0.0",
    "eslint": "~8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "27.2.3",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5",
    "ts-jest": "27.0.5",
    "typescript": "~4.6.2"
  },
  "peerDependencies": {
    "aws-cdk-lib": "2.x",
    "constructs": "10.x"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "tarballDir": "dist"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": "dist/*.tgz"
        }
      ]
    ],
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
}
